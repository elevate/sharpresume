						<xsd:extension base="xsd:string">
							<xsd:attribute name="type" type="xsd:string"/>
							<xsd:attribute name="validFrom" type="AnyDateTimeNaType"/>
							<xsd:attribute name="validTo" type="AnyDateTimeNaType"/>
