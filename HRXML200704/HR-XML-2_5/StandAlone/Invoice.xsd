					<xs:element ref="CorrespondenceLanguage" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="Qualification" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element ref="UserArea" minOccurs="0"/>
