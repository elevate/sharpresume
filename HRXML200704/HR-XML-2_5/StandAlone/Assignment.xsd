	<xsd:element name="RoundingElements">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="Rounding" type="RoundingRuleType" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:sequence/>
				<xsd:element name="TimeRounding" type="TimeRoundingRuleType" minOccurs="0" maxOccurs="unbounded"/>
