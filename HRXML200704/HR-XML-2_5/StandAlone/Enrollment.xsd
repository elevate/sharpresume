							<xsd:simpleType>
								<xsd:union memberTypes="ProductTypeType xStringPatternExtensionType"/>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="CoveredEntity">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
