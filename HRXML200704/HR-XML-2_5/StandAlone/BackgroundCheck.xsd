	<xsd:complexType name="RatingType">
		<xsd:choice>
			<xsd:element name="NumericValue">
				<xsd:complexType>
					<xsd:simpleContent>
						<xsd:extension base="xsd:double">
							<xsd:attribute name="minValue" type="xsd:double"/>
							<xsd:attribute name="maxValue" type="xsd:double"/>
